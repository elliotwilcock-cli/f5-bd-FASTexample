                  }],
                  "class": "Pool"
              },
              "ExchangeVS_ews_pool3": {
                  "loadBalancingMode": "least-connections-member",
                  "members": [
                      {
                          "addressDiscovery": "static",
                          "servicePort":  443,
                          "serverAddresses":  ["172.16.60.107"]
                      }
                  ],
                  "monitors": [{
                       "use": "/ExchangeTenant/ExchApp/ExchangeVS_ews_http_monitor"
                  }],
                  "class": "Pool"
              },
              "ExchangeVS_mapi_pool3": {
                  "loadBalancingMode": "least-connections-member",
                  "members": [
                      {
                          "addressDiscovery": "static",
                          "servicePort":  443,
