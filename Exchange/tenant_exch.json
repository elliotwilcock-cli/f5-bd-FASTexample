                  }],
                  "class": "Pool"
              },
              "ExchangeVS_owa_pool3": {
                  "loadBalancingMode": "least-connections-member",
                  "members": [
                      {
                          "addressDiscovery": "static",
                          "servicePort":  443,
                          "serverAddresses":  ["172.16.60.107"]
                      }
                  ],
                  "monitors": [{
                       "use": "/ExchangeTenant/ExchApp/ExchangeVS_owa_http_monitor"
                  }],
                  "class": "Pool"
              },

              "ExchangeVS_combined_pool_irule3": {
                  "class": "iRule",
                  "iRule": "when HTTP_REQUEST {\n    switch -glob -- [string tolower [HTTP::path]] {\n        \"/microsoft-server-activesync*\" {\n            TCP::idletime 1800\n            pool /ExchangeTenant/ExchApp/ExchangeVS_as_pool3\n            COMPRESS::disable\n            CACHE::disable\n            return\n        }\n        \"/owa*\" {\n            \n            pool /ExchangeTenant/ExchApp/ExchangeVS_owa_pool3\n            return\n        }\n        \"/ecp*\" {\n            \n            pool /ExchangeTenant/ExchApp/ExchangeVS_owa_pool3\n            return\n        }\n        \"/ews*\" {\n            pool /ExchangeTenant/ExchApp/ExchangeVS_ews_pool3\n            COMPRESS::disable\n            CACHE::disable\n            return\n        }\n        \"/oab*\" {\n            pool /ExchangeTenant/ExchApp/ExchangeVS_ews_pool3\n            persist none\n            return\n        }\n        \"/mapi*\" {\n            pool /ExchangeTenant/ExchApp/ExchangeVS_mapi_pool3\n            COMPRESS::disable\n            CACHE::disable\n            return\n         }\n        \"/rpc/rpcproxy.dll*\" {\n            \n            COMPRESS::disable\n            CACHE::disable\n            return\n        }\n        \"/autodiscover*\" {\n            pool /ExchangeTenant/ExchApp/ExchangeVS_ad_pool3\n            persist none\n            return\n        }\n        default {\n            pool /ExchangeTenant/ExchApp/ExchangeVS_owa_pool3\n        }\n    }\n}\nwhen HTTP_RESPONSE {\n    if { ( [HTTP::header exists \"WWW-Authenticate\"] &&\n        [string tolower [HTTP::header values \"WWW-Authenticate\"]] contains \"negotiate\" ) ||\n        ( [HTTP::header exists \"Persistent-Auth\"] &&\n        [string tolower [HTTP::header \"Persistent-Auth\"]] contains \"true\" ) } {\n        ONECONNECT::reuse disable\n        ONECONNECT::detach disable\n        NTLM::disable\n   }\n   if {[HTTP::header exists \"Transfer-Encoding\"]} {\n        HTTP::payload rechunk\n   }\n}"
              },
              "ExchangeVS_owa_redirect_irule3": {
                  "class": "iRule",
                  "iRule": "priority 900\nwhen HTTP_REQUEST {\n    if { ([HTTP::uri] == \"/\") } {\n        HTTP::redirect https://[HTTP::host]/owa/\n    }\n}"
              },
  "webtls": {
        "class": "TLS_Server",
        "certificates": [{
        "certificate": "webcert"
        }]
  },
