      "ExchangeTenant": {
          "class": "Tenant",
          "ExchApp": {
              "class": "Application",
              "template": "https",
              "ExchangeVS_ad_pool3": {
                  "loadBalancingMode": "least-connections-member",
                  "members": [
                      {
                          "addressDiscovery": "static",
                          "servicePort":  443,
                          "serverAddresses":  ["172.16.60.107"]
                      }
                  ],
                  "monitors": [{
                       "use": "/ExchangeTenant/ExchApp/ExchangeVS_ad_http_monitor"
                  }],
